 #include "FPToolkit.c"

int main()
{
  double swidth, sheight, x, y, i, t;
	swidth = 800; sheight = 800;
	G_init_graphics(swidth,sheight);

	double j = 0;
	while(1){
	  
	  G_rgb(0.3,0.3,0.3);
	  G_clear();
	  G_rgb(0,0,0);
	  G_circle(x, y, 50)

	  
	  x = cos(t)* 300 + 400;
	  y = sin(t)* 300 + 400;

	  G_line(400,400,x,y);

	  G_line(x,y,x1,y1)
	  
	  i++;

	}

	int key;
	key = G_wait_key();

}
